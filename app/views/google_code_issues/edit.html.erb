<h1>Editing google_code_issue</h1>

<% form_for(@google_code_issue) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @google_code_issue %> |
<%= link_to 'Back', google_code_issues_path %>
